@font-face {
    font-family: 'Intro';
    src: url('{{ 'IntroRegular.eot' | assets_url }}');
    src: url('{{ 'IntroRegular.eot' | assets_url }}?#iefix') format('eot'),
    url('{{ 'IntroRegular.woff' | assets_url }}') format('woff'),
    url('{{ 'IntroRegular.ttf' | assets_url }}') format('truetype'),
    url('{{ 'IntroRegular.svg' | assets_url }}') format('svg');
    font-weight: normal;
    font-style: normal;
}

